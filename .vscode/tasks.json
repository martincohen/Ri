{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "process",
            "command" : "${workspaceRoot}\\build.bat",
            "problemMatcher": [ "$msCompile" ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-clang",
            "type": "process",
            "command" : "${workspaceRoot}\\build-clang.bat",
            "problemMatcher": [ "$msCompile" ],
        }
    ]
}